			<Menu name="Threshold" click="open-dialog" arg="dlgThreshold"/>
			<Menu name="Gradient Map" disabled="1"/>
			<Menu name="Selective Color" disabled="1"/>
			<Menu type="divider"/>
			<Menu name="Shadow Highlights" disabled="1"/>
			<Menu type="divider"/>
			<Menu name="Desaturate" click="filter-render" arg="grayscale"/>
