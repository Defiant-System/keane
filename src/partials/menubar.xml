		<Menu name="Snap" disabled="1"/>
		<Menu name="Snap To&#8230;" disabled="1"/>
		<Menu type="divider"/>
		<Menu name="Lock Guides" disabled="1"/>
		<Menu name="Clear Guides" disabled="1"/>
		<Menu name="Add Guides&#8230;" disabled="1"/>
		<Menu name="Guides From Layer" disabled="1"/>
		<Menu type="divider"/>
		<Menu name="Clear Slices" disabled="1"/>
		<Menu name="Status Bar" click="toggle-statusbar" check-group="toggle-statusbar" is-checked="1" />
