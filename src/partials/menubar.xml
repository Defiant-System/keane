<MenuBar>
	<Menu name="Keane">
		<Menu name="About Keane" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Quit Keane" shell="win -c"/>
	</Menu>
	<Menu name="File">
		<Menu name="New"/>
		<Menu name="Open&#8230;"/>
		<Menu type="divider"/>
		<Menu name="Close" click="close-file"/>
		<Menu name="Close All"/>
		<Menu name="Save"/>
		<Menu name="Save As&#8230;"/>
		<Menu type="divider"/>
		<Menu name="Export"/>
	</Menu>
	<Menu name="Edit">
		<Menu name="Cut"/>
		<Menu name="Copy"/>
		<Menu name="Paste"/>
		<Menu name="Clear"/>
	</Menu>
	<Menu name="Image">
		<Menu name="Mode">
			<Menu name="RGB" disabled="1"/>
			<Menu name="CMYK" disabled="1"/>
