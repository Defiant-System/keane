		<Menu type="divider"/>
		<Menu name="Reduce Colors" disabled="1"/>
		<Menu name="Vectorize Bitmap" disabled="1"/>
		<Menu type="divider"/>
		<Menu name="Canvas Size" disabled="1"/>
		<Menu name="Image Size" disabled="1"/>
		<Menu name="Transform">
			<Menu name="Rotate 90° Clockwise" disabled="1"/>
			<Menu name="Rotate 90° Counter Clockwise" disabled="1"/>
